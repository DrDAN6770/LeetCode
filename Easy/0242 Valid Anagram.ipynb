   "source": [
    "from collections import Counter\n",
    "class Solution:\n",
    "    def isAnagram(self, s: str, t: str) -> bool:\n",
    "        return Counter(s) == Counter(t)\n",
    "ans = Solution()\n",
    "ans.isAnagram(s = \"rat\", t = \"car\")"
   ]
