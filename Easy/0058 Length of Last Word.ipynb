   "source": [
    "# O(n) O(1)\n",
    "class Solution:\n",
    "    def lengthOfLastWord(self, s: str) -> int:\n",
    "        return len(s.split()[-1])\n",
    "\n",
    "ans = Solution()\n",
    "ans.lengthOfLastWord('Hi Dan  ')"
   ]
